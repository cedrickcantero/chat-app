{
    "compilerOptions": {
      "target": "ES5",
      "lib": ["DOM", "ES2015"],
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noFallthroughCasesInSwitch": true,
      "module": "ESNext",
      "moduleResolution": "Node",
      "isolatedModules": true,
      "jsx": "react-jsx",
      "resolveJsonModule": true,
      "noEmit": true
    },
    "include": ["src"]
  }
  